<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.itf.ReagentsInputListMapper">

<delete id="deleteReagentsInputList">
        delete from reagentsinputlist where rlId=#{rlId};
    </delete>
    <insert id="insertReagentsInputList">
        insert into reagentsinputlist
        values (#{rlId}, #{rlDate}, #{rlFactory}, #{rLReg});
    </insert>
    <select id="selectReagentsInputList" resultType="org.example.pojo.ReagentsInputList">
        SELECT *
        FROM reagentsinputlist
        where reagentsinputlist.rlId = #{rlId};
    </select>
    <update id="updateReagentsInputList">
        update reagentsinputlist
        <trim prefix="set" suffixOverrides=",">
            rlId=#{rlId},
            rlDate=#{rlDate},
            rlFactory=#{rlFactory},
            rLReg=#{rLReg},
        </trim>
        where rlId=#{rlId};
    </update>
</mapper>
